<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="StandardConfig" name="StandardConfig"
		class="org.socraticgrid.spring.tools.configuration.ConfigurationTaskQueue">
		<property name="items">
			<list>
				<bean
					class="org.socraticgrid.spring.tools.configuration.PropertyAssignmentTask">
					<property name="beanName" value="MedicationOrder"></property>
					<property name="property" value="endpoint"></property>
					<property name="value" value="http://172.31.5.104/csp/cpm/OrderFulfillment.Service.FulfillmentPort.cls">
					</property>
				</bean>
			</list>
		</property>
	</bean>
	
	
</beans>
